{
  "extends": "stylelint-config-standard-scss",
  "rules": {
    "declaration-colon-newline-after": "always-multi-line",
    "selector-type-no-unknown": null,
    "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "content-visibility"
          ]
        }
    ]
  }
}
